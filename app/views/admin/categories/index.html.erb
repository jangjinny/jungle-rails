<section class="admin-cate-index">

  <header class="page-header">
    <h1>Admin &raquo; All Categories</h1>
  </header>

  <div class="well">
    <%= link_to '+ New Catgeory', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>

  <div class="panel panel-default category">

    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Category List</th>
        </tr>
      </thead>
      <tbody>
      <% @categories.each do |category|%>
        <tr>
          <td><%= category.name %> </td>
        </tr>
      <% end %>
    </tbody>
    </table>

  </div>

</section>
